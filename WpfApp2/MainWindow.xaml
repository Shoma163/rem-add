<Window x:Class="WpfApp2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <StackPanel>
                <ComboBox Name="comboBox" HorizontalAlignment="Left" Margin="10" Width="150"></ComboBox>
                <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="10" Width="150" />
                <Button Content="Add" Click="Button_Click" HorizontalAlignment="Left" Margin="10" Width="150"></Button>
                <Button Content="Delete" Click="Button_Click_1" HorizontalAlignment="Left" Margin="10" Width="150"></Button>
                <Button Content="Cleare" Click="Button_Click_2" HorizontalAlignment="Left" Margin="10" Width="150"></Button>
            </StackPanel>
        </DockPanel>
    </Grid>
</Window>
